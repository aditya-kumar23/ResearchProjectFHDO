{
  "dataset": "dataset/wifi/r3_wifi.jrl",
  "export_path": "out/demo-decentralised_baselinev10",
  "backend": "decentralised",
  "kpi": true,
  "factor_topic_prefix": "/c_slam/factor_batch",
  "iface_topic_prefix": "/c_slam/iface",
  "use_sim_time": true,
  "quat_order": "wxyz",
  "robust": "cauchy",
  "robust_k": 1.0,
  "impair": {
    "enabled": false,
    "apply_to": ["publisher", "solver"],
    "spec_file": "tools/impair.example.json"
  },
  "startup_delay": 12.0,
  "shutdown_grace": 5.0,
  "solver": {
    "relin_th": 0.06,
    "relin_skip": 6,
    "central_batch_size": 1,
    "central_idle_timeout": 60.0,
    "cpu_affinity": [0, 1],
    "include_potential_outliers": true,
    "emit_factor_acks": true,
    "emit_map_downlink": true,
    "map_topic_prefix": "/c_slam/map",
    "post_input_settle_updates": -1,
    "ddf_rounds": 100,
    "ddf_stable_rounds": 10,
    "ddf_convergence": 0.005,
    "ddf_rot_convergence": 0.005,
    "ddf_factor_idle_timeout": 5.0
  },
  "publisher": {
    "batch_size": 1,
    "time_scale": 5e9,
    "max_sleep": 0.05,
    "idle_gap": 2.0,
    "loop": 1,
    "wait_subscriber_timeout": 25.0,
    "cpu_affinity": [0, 1],
    "include_potential_outliers": true,
    "quat_order": "wxyz",
    "qos_reliability": "reliable",
    "qos_durability": "volatile",
    "qos_depth": 20
  }
}
